type Query {
    firstQuery : String
    secondQuery : String
    fullName(request : RequestFullName) : String
    student(id : Long) : StudentResponse
}

input RequestFullName {
    firstName : String!
    lastName : String
}

type StudentResponse {
	id : Long
	firstName : String
	lastName : String
	email : String
	street : String
	city : String
	learningSubjects(subjectNameFilter : SubjectNameFilter) : [SubjectResponse]
	fullName : String
}

type SubjectResponse {
    id : Long
    subjectName : String
    marksObtained : Float
}

enum SubjectNameFilter{
    All,
    Java,
    MySQL,
    MongoDB
}